<% layout('layout/boilerplate') %>

    <body>
        <h1>All PlanetPins</h1>
        <div>
            <a href="/earthlyExplorer/newPin">Add PlanetPin</a>
        </div>
        <ul>
            <% for(let pin of pins){ %>
                <li><a href="/earthlyExplorer/<%= pin._id %>">
                        <%= pin.title %>
                    </a></li>
                <% }%>
        </ul>
    </body>